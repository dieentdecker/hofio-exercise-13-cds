{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.sap.training00.HOFIOCDSMaster",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZHOFIO_EMPLOYEE_CDS": {
				"uri": "/sap/opu/odata/sap/ZHOFIO_EMPLOYEE_CDS/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml",
					"annotations": [
						"ZHOFIO_EMPLOYEE_CDS_VAN"
					]
				}
			},
			"ZHOFIO_EMPLOYEE_CDS_VAN": {
				"uri": "localService/ZHOFIO_EMPLOYEE_CDS_VAN.xml",
				"type": "ODataAnnotation",
				"settings": {
					"localUri": "localService/ZHOFIO_EMPLOYEE_CDS_VAN.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.sap.training00.HOFIOCDSMaster.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.sap.training00.HOFIOCDSMaster.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZHOFIO_EMPLOYEE_CDS",
				"preload": true
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.sap.training00.HOFIOCDSMaster.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "Master",
					"pattern": "/",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"Master"
					]
				},
				{
					"name": "Customer",
					"pattern": "/customer/{customerid}",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"Customer"
					]
				}
			],
			"targets": {
				"TargetApp": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "App",
					"viewName": "App"
				},
				"Master": {
					"viewType": "XML",
					"viewName": "Master",
					"controlAggregation": "pages"
				},
				"Customer": {
					"viewType": "XML",
					"viewName": "Customer"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zhofio_master/webapp",
		"_version": "1.1.0"
	}
}